{
  "name": "web-template",
  "version": "1.0.0",
  "scripts": {
    "webextension:dev": "multiplerun webextension-dev",
    "webextension:watch": "cross-env TS_NODE_PROJECT=tsconfig.script.json INSPECT_CHROME=9222 node -r ts-node/register -r tsconfig-paths/register scripts/webextension.watch.ts",
    "webextension:build": "cross-env TS_NODE_PROJECT=tsconfig.script.json INSPECT_CHROME=9222 node -r ts-node/register -r tsconfig-paths/register scripts/webextension.build.ts",
    "webextension-test-app:start": "cross-env TS_NODE_PROJECT=tsconfig.script.json INSPECT_CHROME=9222 node -r ts-node/register -r tsconfig-paths/register scripts/webextension-test-app.start.ts",
    "webextension-test-app:build": "cross-env TS_NODE_PROJECT=tsconfig.script.json INSPECT_CHROME=9222 node -r ts-node/register -r tsconfig-paths/register scripts/webextension-test-app.build.ts",
    "storybook:start": "start-storybook",
    "storybook:build": "build-storybook --output-dir out/storybook",
    "build": "yarn run webextension:build",
    "test": "jest --colors",
    "coverage": "jest --coverage",
    "precommit": "lint-staged"
  },
  "multiplerun": {
    "webextension-dev": [
      "yarn run webextension:watch",
      "yarn run webextension-test-app:start"
    ]
  },
  "lint-staged": {
    "src/{**/*,*}/*.{ts,tsx}": [
      "jest --colors --findRelatedTests"
    ]
  },
  "dependencies": {
    "@anchor-protocol/anchor.js": "^0.1.18",
    "@anchor-protocol/notation": "^0.1.0",
    "@anchor-protocol/types": "^0.1.0",
    "@apollo/client": "^3.3.13",
    "@terra-dev/use-map": "^0.1.0",
    "@terra-money/terra.js": "^1.6.0",
    "big.js": "^6.0.3",
    "bowser": "^2.11.0",
    "crypto-browserify": "^3.12.0",
    "crypto-js": "^4.0.0",
    "d3-ease": "^2.0.0",
    "fast-deep-equal": "^3.1.3",
    "graphql": "^15.5.0",
    "process": "^0.11.10",
    "react-intl": "^5.15.5",
    "react-router-dom": "^5.2.0",
    "react-spinners-kit": "^1.9.1",
    "react-spring": "^9.0.0",
    "rxjs": "^6.6.7",
    "styled-components": "^5.2.3",
    "use-locale": "^0.3.0",
    "use-resize-observer": "^7.0.0",
    "use-state-ref": "^1.0.0",
    "webextension-polyfill-ts": "^0.25.0"
  },
  "devDependencies": {
    "@rocket-scripts/web": "^2.1.2",
    "@sentry/browser": "^6.2.4",
    "@sentry/tracing": "^6.2.4",
    "@storybook/addon-essentials": "^6.2.1",
    "@storybook/addon-storysource": "^6.2.1",
    "@storybook/react": "^6.2.1",
    "@types/big.js": "^6.0.2",
    "@types/chrome": "^0.0.133",
    "@types/crypto-js": "^4.0.1",
    "@types/d3-ease": "^2.0.0",
    "@types/jest": "^26.0.22",
    "@types/node": "^14.14.37",
    "@types/puppeteer": "^5.4.3",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "@types/react-router-dom": "^5.1.7",
    "@types/styled-components": "^5.1.9",
    "@types/webpack-env": "^1.16.0",
    "cross-env": "^7.0.3",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "multiplerun": "^2.0.1",
    "puppeteer": "^6.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "stream-browserify": "^3.0.0",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.2.3",
    "web-vitals": "^1.1.1"
  }
}
