{
  "name": "apps",
  "version": "1.0.0",
  "scripts": {
    "webextension:dev": "multiplerun webextension-dev",
    "webextension:watch": "cross-env TS_NODE_PROJECT=tsconfig.script.json INSPECT_CHROME=9222 node -r ts-node/register -r tsconfig-paths/register scripts/webextension.watch.ts",
    "webextension:build": "cross-env TS_NODE_PROJECT=tsconfig.script.json INSPECT_CHROME=9222 node -r ts-node/register -r tsconfig-paths/register scripts/webextension.build.ts",
    "webextension-reloader:watch": "cross-env TS_NODE_PROJECT=tsconfig.script.json INSPECT_CHROME=9222 node -r ts-node/register -r tsconfig-paths/register scripts/webextension-reloader.watch.ts",
    "webextension-test-app:start": "vite --config webextension-test-app.vite.ts",
    "webextension-test-app:build": "tsc --noEmit && vite build --config webextension-test-app.vite.ts",
    "webextension:browser": "cross-env TS_NODE_PROJECT=tsconfig.script.json INSPECT_CHROME=9222 node -r ts-node/register -r tsconfig-paths/register scripts/webextension.browser.ts",
    "storybook:start": "start-storybook",
    "storybook:build": "build-storybook --output-dir out/storybook",
    "build": "yarn run webextension:build",
    "start": "yarn run webextension:dev",
    "test": "jest --colors",
    "pack": "rocket-punch build --strict",
    "publish": "rocket-punch publish",
    "coverage": "jest --coverage",
    "precommit": "lint-staged"
  },
  "sourceType": "module",
  "multiplerun": {
    "webextension-dev": [
      [
        "yarn run tsc --watch --noEmit",
        "yarn run webextension-test-app:start"
      ],
      [
        "yarn run webextension:watch",
        {
          "command": "yarn run webextension:browser",
          "wait": 30000
        }
      ]
    ]
  },
  "lint-staged": {
    "src/{**/*,*}/*.{ts,tsx}": [
      "jest --colors --findRelatedTests"
    ]
  },
  "dependencies": {
    "@apollo/client": "^3.3.16",
    "@handbook/code-block": "^1.1.0",
    "@ledgerhq/hw-transport": "^6.3.0",
    "@ledgerhq/hw-transport-webusb": "^6.3.0",
    "@material-ui/core": "^4.11.3",
    "@material-ui/icons": "^4.11.2",
    "@rx-stream/animation": "^0.4.0",
    "@rx-stream/pipe": "^0.4.0",
    "@rx-stream/react": "^0.4.0",
    "@terra-dev/use-map": "^0.10.0",
    "@terra-money/key-utils": "^1.0.2",
    "@terra-money/ledger-terra-js": "^1.1.0",
    "@terra-money/terra.js": "^1.8.0 || ^2.0.0",
    "big.js": "^6.1.1",
    "bowser": "^2.11.0",
    "crypto-browserify": "^3.12.0",
    "crypto-js": "^4.0.0",
    "d3-ease": "^2.0.0",
    "d3-interpolate": "^2.0.1",
    "d3-timer": "^2.0.0",
    "fast-deep-equal": "^3.1.3",
    "fix-hmr": "^1.0.2",
    "graphql": "^15.5.0",
    "post-message-stream": "^3.0.0",
    "process": "^0.11.10",
    "react-intl": "^5.17.4",
    "react-router-dom": "^5.2.0",
    "react-spinners-kit": "^1.9.1",
    "react-spring": "^8.0.27",
    "rxjs": "^7.0.0",
    "secp256k1": "^4.0.2",
    "styled-components": "^5.2.3",
    "use-locale": "^0.3.0",
    "use-resize-observer": "^7.0.0",
    "use-state-ref": "^1.0.0",
    "webextension-polyfill-ts": "^0.25.0"
  },
  "devDependencies": {
    "@mdx-js/mdx": "^1.6.22",
    "@mdx-js/react": "^1.6.22",
    "@rocket-scripts/react-preset": "^4.0.0-alpha.3",
    "@rocket-scripts/read-package-alias": "^4.0.0-alpha.3",
    "@rocket-scripts/webpack-fns": "^4.0.0-alpha.3",
    "@sentry/browser": "^6.3.5",
    "@sentry/tracing": "^6.3.5",
    "@storybook/addon-essentials": "^6.2.9",
    "@storybook/addon-storysource": "^6.2.9",
    "@storybook/react": "^6.2.9",
    "@types/big.js": "^6.0.2",
    "@types/chrome": "^0.0.136",
    "@types/crypto-js": "^4.0.1",
    "@types/d3-ease": "^2.0.0",
    "@types/d3-interpolate": "^2.0.0",
    "@types/d3-timer": "^2.0.0",
    "@types/fs-extra": "^9.0.12",
    "@types/jest": "^26.0.23",
