    "@types/react-router-dom": "^5.1.7",
    "@types/rimraf": "^3.0.0",
    "@types/secp256k1": "^4.0.3",
    "@types/styled-components": "^5.1.9",
    "@types/w3c-web-usb": "^1.0.5",
    "@types/webpack-env": "^1.16.0",
    "@vitejs/plugin-react-refresh": "^1.3.6",
    "cross-env": "^7.0.3",
    "fs-extra": "^10.0.0",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "multiplerun": "^2.0.1",
    "puppeteer": "^8.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rimraf": "^3.0.2",
    "rocket-punch": "^3.0.1",
    "stream-browserify": "^3.0.0",
    "ts-node": "^9.1.1",
