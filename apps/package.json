{
  "name": "apps",
  "version": "1.0.0",
  "scripts": {
    "webextension:dev": "multiplerun webextension-dev",
    "webextension:watch": "cross-env TS_NODE_PROJECT=tsconfig.script.json INSPECT_CHROME=9222 node -r ts-node/register -r tsconfig-paths/register scripts/webextension.watch.ts",
    "webextension:build": "cross-env TS_NODE_PROJECT=tsconfig.script.json INSPECT_CHROME=9222 node -r ts-node/register -r tsconfig-paths/register scripts/webextension.build.ts",
    "webextension-reloader:watch": "cross-env TS_NODE_PROJECT=tsconfig.script.json INSPECT_CHROME=9222 node -r ts-node/register -r tsconfig-paths/register scripts/webextension-reloader.watch.ts",
    "webextension-test-app:start": "vite --config webextension-test-app.vite.ts",
    "webextension-test-app:build": "tsc --noEmit && vite build --config webextension-test-app.vite.ts",
    "webextension:browser": "cross-env TS_NODE_PROJECT=tsconfig.script.json INSPECT_CHROME=9222 node -r ts-node/register -r tsconfig-paths/register scripts/webextension.browser.ts",
    "storybook:start": "start-storybook --static-dir ./public/common --port 60055",
    "storybook:build": "build-storybook --static-dir ./public/common --output-dir out/storybook",
    "build": "yarn run webextension:build",
    "start": "yarn run webextension:dev",
    "test": "jest --colors",
    "pack": "rocket-punch build --strict",
    "publish": "rocket-punch publish",
    "coverage": "jest --coverage",
    "precommit": "lint-staged"
  },
  "sourceType": "module",
  "multiplerun": {
    "webextension-dev": [
      [
        "yarn run tsc --watch --noEmit",
        "yarn run webextension-test-app:start"
      ],
      [
        "yarn run webextension:watch",
        {
          "command": "yarn run webextension:browser",
          "wait": 30000
        }
      ]
    ]
  },
  "lint-staged": {
    "src/{**/*,*}/*.{ts,tsx}": [
      "jest --colors --findRelatedTests"
    ]
  },
  "dependencies": {
