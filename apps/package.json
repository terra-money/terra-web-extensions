{
  "name": "apps",
  "version": "1.0.0",
  "scripts": {
    "webextension:dev": "multiplerun webextension-dev",
    "webextension:dev-legacy": "multiplerun webextension-dev-legacy",
    "webextension:watch": "cross-env TS_NODE_PROJECT=tsconfig.script.json INSPECT_CHROME=9222 node -r ts-node/register -r tsconfig-paths/register scripts/webextension.watch.ts",
    "webextension:build": "cross-env TS_NODE_PROJECT=tsconfig.script.json INSPECT_CHROME=9222 node -r ts-node/register -r tsconfig-paths/register scripts/webextension.build.ts",
    "webextension-reloader:watch": "cross-env TS_NODE_PROJECT=tsconfig.script.json INSPECT_CHROME=9222 node -r ts-node/register -r tsconfig-paths/register scripts/webextension-reloader.watch.ts",
    "webextension-test-app:start": "cross-env TS_NODE_PROJECT=tsconfig.script.json INSPECT_CHROME=9222 node -r ts-node/register -r tsconfig-paths/register scripts/webextension-test-app.start.ts",
    "webextension-test-app:build": "cross-env TS_NODE_PROJECT=tsconfig.script.json INSPECT_CHROME=9222 node -r ts-node/register -r tsconfig-paths/register scripts/webextension-test-app.build.ts",
    "webextension-legacy-anchor:start": "cross-env TS_NODE_PROJECT=tsconfig.script.json INSPECT_CHROME=9222 node -r ts-node/register -r tsconfig-paths/register scripts/webextension-legacy-anchor.start.ts",
    "storybook:start": "start-storybook",
    "storybook:build": "build-storybook --output-dir out/storybook",
    "build": "yarn run webextension:build",
    "test": "jest --colors",
    "pack": "rocket-punch build --strict",
    "publish": "rocket-punch publish",
    "coverage": "jest --coverage",
    "precommit": "lint-staged"
  },
  "sourceType": "module",
  "multiplerun": {
    "webextension-dev": [
      "yarn run webextension:watch",
      "yarn run webextension-test-app:start"
    ],
    "webextension-dev-legacy": [
      "yarn run webextension:watch",
      "yarn run webextension-legacy-anchor:start"
    ]
  },
  "lint-staged": {
    "src/{**/*,*}/*.{ts,tsx}": [
      "jest --colors --findRelatedTests"
    ]
  },
  "dependencies": {
    "@anchor-protocol/anchor.js": "^1.0.1",
    "@anchor-protocol/notation": "^0.10.0",
    "@anchor-protocol/types": "^0.10.0",
    "@apollo/client": "^3.3.16",
    "@handbook/code-block": "^1.1.0",
    "@material-ui/core": "^4.11.3",
    "@material-ui/icons": "^4.11.2",
    "@rx-stream/animation": "^0.4.0",
    "@rx-stream/pipe": "^0.4.0",
    "@rx-stream/react": "^0.4.0",
    "@terra-dev/use-map": "^0.10.0",
    "@terra-money/key-utils": "^1.0.2",
    "@terra-money/terra.js": "^1.7.0",
    "big.js": "^6.1.1",
    "bowser": "^2.11.0",
    "crypto-browserify": "^3.12.0",
    "crypto-js": "^4.0.0",
    "d3-ease": "^2.0.0",
    "d3-interpolate": "^2.0.1",
    "d3-timer": "^2.0.0",
    "fast-deep-equal": "^3.1.3",
    "graphql": "^15.5.0",
    "post-message-stream": "^3.0.0",
    "process": "^0.11.10",
    "react-intl": "^5.17.4",
    "react-router-dom": "^5.2.0",
    "react-spinners-kit": "^1.9.1",
    "react-spring": "^8.0.27",
    "rxjs": "^7.0.0",
    "styled-components": "^5.2.3",
    "use-locale": "^0.3.0",
    "use-resize-observer": "^7.0.0",
    "use-state-ref": "^1.0.0",
    "webextension-polyfill-ts": "^0.25.0"
  },
  "devDependencies": {
    "@rocket-scripts/web": "^3.1.1",
    "@sentry/browser": "^6.3.5",
    "@sentry/tracing": "^6.3.5",
    "@storybook/addon-essentials": "^6.2.9",
    "@storybook/addon-storysource": "^6.2.9",
    "@storybook/react": "^6.2.9",
    "@types/big.js": "^6.0.2",
    "@types/chrome": "^0.0.136",
    "@types/crypto-js": "^4.0.1",
    "@types/d3-ease": "^2.0.0",
    "@types/d3-interpolate": "^2.0.0",
    "@types/d3-timer": "^2.0.0",
    "@types/jest": "^26.0.23",
    "@types/node": "^15.0.1",
    "@types/react": "^17.0.4",
    "@types/react-dom": "^17.0.3",
    "@types/react-router-dom": "^5.1.7",
    "@types/rimraf": "^3.0.0",
    "@types/styled-components": "^5.1.9",
    "@types/webpack-env": "^1.16.0",
    "cross-env": "^7.0.3",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "multiplerun": "^2.0.1",
    "puppeteer": "^8.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rimraf": "^3.0.2",
    "rocket-punch": "^2.5.0",
    "stream-browserify": "^3.0.0",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.2.4",
    "web-vitals": "^1.1.1"
  }
}
